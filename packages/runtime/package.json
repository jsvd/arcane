{
  "name": "@arcane-engine/runtime",
  "version": "0.11.0",
  "deprecated": "Use cargo install arcane-engine instead. Runtime is now embedded in the CLI binary.",
  "description": "Agent-native 2D game engine runtime - TypeScript APIs for state management, rendering, and game logic",
  "type": "module",
  "scripts": {
    "prepack": "rm -f src && cp -r ../../runtime src",
    "postpack": "rm -rf src && ln -s ../../runtime src"
  },
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": "./index.ts",
    "./state": "./src/state/index.ts",
    "./rendering": "./src/rendering/index.ts",
    "./ui": "./src/ui/index.ts",
    "./physics": "./src/physics/index.ts",
    "./pathfinding": "./src/pathfinding/index.ts",
    "./systems": "./src/systems/index.ts",
    "./agent": "./src/agent/index.ts",
    "./testing": "./src/testing/harness.ts",
    "./tweening": "./src/tweening/index.ts",
    "./particles": "./src/particles/index.ts",
    "./scenes": "./src/scenes/index.ts",
    "./persistence": "./src/persistence/index.ts",
    "./procgen": "./src/procgen/index.ts",
    "./game": "./src/game/index.ts",
    "./input": "./src/input/index.ts"
  },
  "files": [
    "src",
    "index.ts",
    "README.md"
  ],
  "keywords": [
    "game-engine",
    "2d",
    "gamedev",
    "typescript",
    "ai-agent",
    "agent-native",
    "state-management",
    "ecs"
  ],
  "author": "Arcane Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsvd/arcane.git",
    "directory": "packages/runtime"
  },
  "bugs": {
    "url": "https://github.com/jsvd/arcane/issues"
  },
  "homepage": "https://github.com/jsvd/arcane#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
